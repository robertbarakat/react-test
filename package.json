{
  "name": "react-test",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bcrypt": "^3.0.3",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.2.1",
    "jsonwebtoken": "^8.4.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-intl": "^2.8.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "reactstrap": "^7.1.0",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2",
    "styled-components": "^4.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext js,jsx",
    "lintjsx": "eslint src --ext jsx",
    "build-preprod": "npm run build && rm ./build/static/**/*.map && rm -rf ../back/build/* && mv ./build/* ../back/build/",
    "precommit": "npm run lint"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
      "es6": true,
      "browser": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "react/jsx-filename-extension": [
        "js",
        "jsx"
      ],
      "jsx-a11y/href-no-hash": "off",
      "jsx-a11y/mouse-events-have-key-events": "off",
      "react/prop-types": "off",
      "no-shadow": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/no-noninteractive-element-interactions": "off"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint-config-airbnb": "^17.1.0",
    "node-gyp": "^3.8.0"
  }
}
